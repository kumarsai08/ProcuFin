<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Order_recieved_back</fullName>
    <active>true</active>
    <errorConditionFormula>AND(ISPICKVAL(PRIORVALUE(  Status__c ),&quot;Order Recieved&quot;),(ISPICKVAL( Status__c ,&quot;Order Placed&quot;)))</errorConditionFormula>
    <errorMessage>The Order is already RECIEVED, Can&apos;t change it&apos;s STATUS.</errorMessage>
</ValidationRule>
